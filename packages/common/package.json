{
  "name": "@repo/common",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    },
    "./lib/browser": {
      "types": "./dist/lib/browser/index.d.ts",
      "import": "./dist/lib/browser/index.js"
    },
    "./lib/node": {
      "types": "./dist/lib/node/index.d.ts",
      "import": "./dist/lib/node/index.js"
    },
    "./classes/*": {
      "types": "./dist/classes/*.d.ts",
      "import": "./dist/classes/*.js"
    },
    "./interfaces/*": {
      "types": "./dist/interfaces/*.d.ts",
      "import": "./dist/interfaces/*.js"
    },
    "./external/*": {
      "types": "./dist/external/*.d.ts",
      "import": "./dist/external/*.js"
    },
    "./next/*": {
      "types": "./dist/next/*.d.ts",
      "import": "./dist/next/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "dist/lib/*"
      ],
      "types": [
        "dist/types/index.d.ts"
      ],
      "types/*": [
        "dist/types/*"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp -r types dist/types",
    "lint": "eslint --fix",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "dayjs": "^1.11.13",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "nanoid": "^5.0.4",
    "next": "15.1.1",
    "querystring": "0.2.0",
    "redis": "^5.7.0",
    "ts-node": "10.9.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.1"
  },
  "peerDependencies": {
    "@react-google-maps/api": "^2.20.5",
    "@types/lodash": "^4.17.15",
    "lodash": "^4.17.21",
    "typescript": "5.8.2"
  }
}
